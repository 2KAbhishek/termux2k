<?xml version="1.0" encoding="utf-8"?>
<!-- Full guide on how to edit the layout: https://github.com/Julow/Unexpected-Keyboard/tree/master/doc/Custom-layouts.md -->
<keyboard name=" Qwerty Dev Compact" script="latin">
  <row>
    <key c="q" nw="esc" ne="1" sw="!"/>
    <key c="w" ne="2" sw="@"/>
    <key c="e" ne="3" sw="#"/>
    <key c="r" ne="4" sw="$"/>
    <key c="t" ne="5" sw="%"/>
    <key c="y" ne="6" sw="^"/>
    <key c="u" ne="7" sw="&amp;"/>
    <key c="i" ne="8" sw="*"/>
    <key c="o" ne="9" sw="("/>
    <key c="p" ne="0" sw=")"/>
  </row>
  <row>
    <key shift="0.5" c="a" nw="tab" ne="~" sw="`"/>
    <key c="s" sw="-" ne="_"/>
    <key c="d" sw="=" ne="+"/>
    <key c="f" sw="[" ne="{"/>
    <key c="g" sw="]" ne="}"/>
    <key c="h" sw="/" ne="="/>
    <key c="j" sw=";" ne=":"/>
    <key c="k" sw="'" ne="&quot;"/>
    <key c="l" sw="|" ne="\?"/>
  </row>
  <row>
    <key width="1.5" c="shift" ne="tab" nw="capslock"/>
    <key c="z" sw="&lt;" ne=","/>
    <key c="x" sw="&gt;" ne="."/>
    <key c="c" sw="~" ne="â‚¹"/>
    <key c="v" sw="|" ne="\"/>
    <key c="b" sw="!" ne="\?"/>
    <key c="n" sw="-" ne="_"/>
    <key c="m" sw="," ne="."/>
    <key width="1.5" c="backspace" nw="esc" ne="delete"/>
  </row>
</keyboard>
